{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://blog.senditfax.com"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog.senditfax.com/:path*"
    },
    {
      "source": "/:year/:month/:day/:slug",
      "destination": "https://blog.senditfax.com/:year/:month/:day/:slug"
    }
  ]
}