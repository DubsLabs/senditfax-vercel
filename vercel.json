{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://blog.senditfax.com"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog.senditfax.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/blog/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}